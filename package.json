{
  "name": "@cybozu/eslint-config",
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "repository": "cybozu/eslint-config",
  "homepage": "https://github.com/cybozu/eslint-config",
  "bugs": "https://github.com/cybozu/eslint-config/issues",
  "version": "25.0.0",
  "description": "ESLint rules for Cybozu",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./presets/base": "./presets/base.js",
    "./presets/css-baseline": "./presets/css-baseline.js",
    "./presets/es5": "./presets/es5.js",
    "./presets/es5-prettier": "./presets/es5-prettier.js",
    "./presets/kintone-customize": "./presets/kintone-customize.js",
    "./presets/kintone-customize-es5": "./presets/kintone-customize-es5.js",
    "./presets/kintone-customize-es5-prettier": "./presets/kintone-customize-es5-prettier.js",
    "./presets/kintone-customize-prettier": "./presets/kintone-customize-prettier.js",
    "./presets/node": "./presets/node.js",
    "./presets/node-prettier": "./presets/node-prettier.js",
    "./presets/node-typescript-prettier": "./presets/node-typescript-prettier.js",
    "./presets/prettier": "./presets/prettier.js",
    "./presets/react": "./presets/react.js",
    "./presets/react-prettier": "./presets/react-prettier.js",
    "./presets/react-typescript": "./presets/react-typescript.js",
    "./presets/react-typescript-prettier": "./presets/react-typescript-prettier.js",
    "./presets/typescript": "./presets/typescript.js",
    "./presets/typescript-prettier": "./presets/typescript-prettier.js"
  },
  "files": [
    "lib",
    "globals",
    "presets",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint **/*.js",
    "test": "mocha --timeout 5000 \"test/**/*-test.js\""
  },
  "keywords": [
    "eslint"
  ],
  "author": "cybozu",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^19.2.7",
    "eslint": "^9.39.1",
    "mocha": "^11.7.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "react": "^19.2.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": "^8.56.0 || ^9.0.0-0",
    "typescript": ">=4.7.5 || ^5.0.0"
  },
  "dependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin-ts": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "typescript-eslint": "^8.49.0"
  },
  "packageManager": "pnpm@10.25.0",
  "devEngines": {
    "packageManager": {
      "name": "pnpm",
      "onFail": "error"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
